START
    apprenants = ["Julie", "Stéphane", "Mostapha", "Claudiu", "Son"]
        WRITE apprenants[1] + " EST DANS LA CLASSE DE DEV"
END

++++
\\ Module principal
DÉBUT

apprenants = ["David", "Justine", "Valentin","Axel", "Redouane"]
competence = ["html", "css", "javascript", "php", "Java"]  

POUR nombre = 1 JUSQU'A 5 FAIRE
    ECRIRE apprenants [nombre], "est dans la calsse de dev " + competence [nombre]
FINPOUR
FIN  

++++
\\ Module principal

START
    APPRENANTS = [["David", "Justine", "Valentin","Axel", "Redouane"], ["Julie", "Stéphane", "Mostapha", "Claudiu", "Son"]]
  
    FOR i = 1 TO 2 DO
        FOR y = 1 TO 5 DO
            WRITE APPRENANTS[i][y] 
        ENDFOR 
    ENDFOR
END  

++++
\\ Module principal
DÉBUT

nombres = [[18,55], [29,13], [-2,15],[66,44]]  
    POUR chiffres = 1 JUSQU'A 4 FAIRE
        ECRIRE max(nombres[chiffres])       
    FINPOUR
FIN  


++++
\\ Module principal

START
    APPRENANTS = [["David", "Justine", "Valentin","Axel", "Redouane"], ["Julie", "Stéphane", "Mostapha", "Claudiu", "Son"]]
  
    FOR i = 1 TO 1 DO
        
            WRITE APPRENANTS[i] + " EST CHEZ LES 2024" 
    ENDFOR 
    
        FOR y = 2 TO 2 DO
            WRITE APPRENANTS[y] + " EST CHEZ LES 2023"
        ENDFOR
END  

\\ ex 2

DÉBUT

APPRENANTS = [["David", "Justine", "Valentin","Axel", "Redouane"], ["Julie", "Stéphane", "Mostapha", "Claudiu", "Son"]]

POUR i = 1 JUSQU'A 1 FAIRE
ECRIRE APPRENANTS[i] + " EST CHEZ LES 2024"
FINPOUR 

POUR y = 2 JUSQU'A 2 FAIRE
       
ECRIRE APPRENANTS[y] + " EST CHEZ LES 2023"
FINPOUR

FIN 


++++

\\ Module principal
DÉBUT

LIRE PRENOM
ECRIRE HELLO(PRENOM) 

FIN 

\\ Module auxiliaire HELLO
ENTRER PRENOM

    ECRIRE
     
RETOURNER "Bonjour et bienvenue " + PRENOM

++++++
\\ Module principal
DÉBUT
ECRIRE "DONNE A"
LIRE NB1
ECRIRE "DONNE B"
LIRE NB2  
ECRIRE AB(NB1,NB2) 

FIN 


\\ Module auxiliaire AB
ENTRER A,B

SI A > B ALORS
    REPONSE = "0"
SINON
    SI B > A ALORS
        REPONSE = "1"
FINSI
    FINSI

RETOURNER REPONSE
++++




