\\ Main module
START

    NOMBRES = [[18,55], [29,13], [-2,15],[66,44]]
    WRITE MAX(NOMBRES)
END


++++
\\ Module principal

START
    APPRENANTS = [["David", "Justine", "Valentin","Axel", "Redouane"], ["Julie", "Stéphane", "Mostapha", "Claudiu", "Son"]]
  
    FOR i = 1 TO 1 DO
        
            WRITE APPRENANTS[i] + " EST CHEZ LES 2024" 
    ENDFOR 
    
        FOR y = 2 TO 2 DO
            WRITE APPRENANTS[y] + " EST CHEZ LES 2023"
        ENDFOR
END  


