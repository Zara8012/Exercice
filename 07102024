\\ Main module
START
    APPRENANTS = ["Julie", "Stéphane", "Mostapha", "Claudiu", "Son"]
    COMPETENCES =  [" HTML" , " CSS" , " JAVASCRIPT" , " PHP" , " JAVA"]    
        WRITE apprenants[1] + COMPETENCES [5]
END

++++
\\ Module principal

START
    APPRENANTS = [["David", "Justine", "Valentin","Axel", "Redouane"], ["Julie", "Stéphane", "Mostapha", "Claudiu", "Son"]]
  
    FOR i = 1 TO 2 DO
        FOR y = 1 TO 5 DO
            WRITE APPRENANTS[i][y] 
        ENDFOR 
    ENDFOR
END  

